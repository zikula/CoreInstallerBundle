{
    "name": "zikula/coreinstaller-bundle",
    "type": "symfony-bundle",
    "description": "An application to install or upgrade the Zikula Core.",
    "keywords": [ "zikula" ],
    "homepage": "https://ziku.la",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Heydenburg",
            "email": "info@ziku.la"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "zikula/core-bundle": "3.*",
        "symfony/contracts": "5.*",
        "symfony/console": "5.*",
        "symfony/form": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/intl": "5.*",
        "symfony/mailer": "5.*",
        "symfony/requirements-checker": "1.*",
        "symfony/security": "1.*",
        "symfony/string": "1.*",
        "symfony/validator": "1.*",
        "symfony/yaml": "1.*",
        "matthiasnoback/symfony-console-form": "4.*",
        "zikula/wizard": "3.*"
    },
    "autoload": {
        "psr-4": { "Zikula\\Bundle\\CoreInstallerBundle\\": "" }
    }
}
